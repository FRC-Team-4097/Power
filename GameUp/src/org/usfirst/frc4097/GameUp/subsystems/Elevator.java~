    public void joystickInput(Joystick main){
    	if (main.getRawButton(3)){
    		linearMotor.set(1);
    	}
    	else if (main.getRawButton(4)){
    		linearMotor.set(-1);
    	}
    	else if (main.getRawButton(2)){
    		linearMotor.set(-1);
    	}
    	else{
    		linearMotor.stopMotor();
    	}
    }
